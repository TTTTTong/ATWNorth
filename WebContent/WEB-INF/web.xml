<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>ATWNorth</display-name>
  <session-config>
    <session-timeout>30</session-timeout>
  </session-config>
  <servlet>
    <servlet-name>action</servlet-name>
    <servlet-class>org.apache.struts.action.ActionServlet</servlet-class>
    <init-param>
      <param-name>config</param-name>
      <param-value>/WEB-INF/struts-config.xml</param-value>
    </init-param>
  </servlet>
  <servlet>
    <servlet-name>buyServlet</servlet-name>
    <servlet-class>Servlet.BuyServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>deleteOrderServlet</servlet-name>
    <servlet-class>Servlet.DeleteOrderServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>logoutServlet</servlet-name>
    <servlet-class>Servlet.LogoutServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>submitOrderServlet</servlet-name>
    <servlet-class>Servlet.SubmitOrderServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GetGoodsInfoServlet</servlet-name>
    <servlet-class>Servlet.GetGoodsInfoServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeliverServlet</servlet-name>
  	<servlet-class>Servlet.manager.DeliverServlet</servlet-class>
  </servlet>
  
  
  <servlet-mapping>
    <servlet-name>action</servlet-name>
    <url-pattern>*.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>buyServlet</servlet-name>
    <url-pattern>/BuyServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>deleteOrderServlet</servlet-name>
    <url-pattern>/DeleteOrderServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>logoutServlet</servlet-name>
    <url-pattern>/LogoutServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>submitOrderServlet</servlet-name>
    <url-pattern>/SubmitOrderServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GetGoodsInfoServlet</servlet-name>
    <url-pattern>/GetGoodsInfoServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeliverServlet</servlet-name>
  	<url-pattern>/DeliverServlet</url-pattern>
  </servlet-mapping>
</web-app>